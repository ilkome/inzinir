extends _layout


block append content
	.main-width.main-wrap
		.pathway
			ul
				li: a(href="#") Рынок услуг
				li >
				li Организации
		//------------------------------------------------
		.page-name
			h1 Организации
			p Рынок услуг организаций
		//------------------------------------------------
		.main-full
			.search-con
				form(action="#")
					input(type="text", placeholder="Быстрый поиск по организациям").inp-text
					input(type="submit", value="").inp-submit
			.box-articles
				.heading
					.heading-left_sort-fl 
						| Сортировать по:						
						a(href="#")
							| НТП
							span
						a(href="#") 
							| Оборудования
							span 
						a(href="#").active 
							| Услуг
							span 
						a(href="#") 
							| ПО
							span
						a(href="#") 
							| РИД
							span
						a(href="#") 
							| Оценок пользователей
							span  
				
				mixin item(name, pic, rating)
					.item-3col.js-object-box
						.iprev
							img(src="img/content/#{pic}", alt="")
							a(href="#").feedback Связаться
						.icon
							.iname
								a(href="#")= name
							div.iobjects
								a(href="#").js-show-objects Предложения организации
							.itext.lh-small
								p Оборудование: 78% <br>
									| Услуги: 67% <br>
									| Программное обеспечние: 68% <br> 
									| Объекты научного технического потенциала: - 28% <br> 
									| Результаты интеллектуальной деятельности: 64% <br>
						.iconright
							.graph-name Рейтинг НТП #{rating} %
							.graph-box
								.graph-inner(style='width:#{rating}%;')
							.irating.irating_margin
								ul
									li.star
									li.star
									li.star
									li
									li
							.imeta
								.comments
